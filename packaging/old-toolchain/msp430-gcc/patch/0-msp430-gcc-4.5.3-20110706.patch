+.endfunc
+#endif
+
+#if defined (L_cmpdi2)
+	.global	__cmpdi2
+	.func	__cmpdi2
+__cmpdi2:
+	sub	ARGP_OFFSET(r1), r12
+	subc	2+ARGP_OFFSET(r1), r13
+	subc	4+ARGP_OFFSET(r1), r14
+	subc	6+ARGP_OFFSET(r1), r15
+	jge	.L_ge
+	mov	#0, r15		; a < b return 0
+	ret
+	
+.L_ge:
+	bis	r12, r15
+	bis	r13, r15
+	bis	r14, r15
+	tst	r15
+	jz	.L_eq
+
+	mov	#2, r15		; a > b return 2
+	ret
+.L_eq:
+	mov	#1, r15		; a == b return 1
+	ret
+.endfunc
+#endif
+
+#if defined (L_ucmpdi2)
+	
+	.global	__ucmpdi2
+	.func	__ucmpdi2
+__ucmpdi2:
+	sub	ARGP_OFFSET(r1), r12
+	subc	2+ARGP_OFFSET(r1), r13
+	subc	4+ARGP_OFFSET(r1), r14
+	subc	6+ARGP_OFFSET(r1), r15
+	jhs	.L_ge
+	mov	#0, r15		; a < b return 0
+	ret
+	
+.L_ge:
+	bis	r12, r15
+	bis	r13, r15
+	bis	r14, r15
+	tst	r15
+	jz	.L_eq
+
+	mov	#2, r15		; a > b return 2
