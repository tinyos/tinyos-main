+int
+zero_shifted (rtx x)
+{
+  int r = 0;
+
+  if (GET_CODE (x) == MEM &&
+      GET_CODE (XEXP (x, 0)) == REG
+      && REGNO (XEXP (x, 0)) != STACK_POINTER_REGNUM
+      && REGNO (XEXP (x, 0)) != HARD_FRAME_POINTER_REGNUM
+      /* the following is Ok, cause we do not corrupt r4 within ISR */
+      /*&& REGNO(XEXP (x,0)) != ARG_POINTER_REGNUM */ )
+    {
+      r = 1;
+    }
+  return r;
+}
