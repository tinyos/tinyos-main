+#if defined (L_cmpsi2)
+	
+	.global	__cmpsi2
+	.func	__cmpsi2
+__cmpsi2:
+	sub	r12, r14
+	subc	r13, r15
+	jge	.L_ge
+	mov	#0, r15		; a < b return 0
+	ret
+	
+.L_ge:
+	bis	r14, r15
+	tst	r15
+	jz	.L_eq
+
+	mov	#2, r15		; a > b return 2
+	ret
+.L_eq:
+	mov	#1, r15		; a == b return 1
+	ret
+.endfunc
+#endif
+
+#if defined (L_ucmpsi2)
+	
+	.global	__ucmpsi2
+	.func	__ucmpsi2
+__ucmpsi2:
