  event void SerialControl.startDone(error_t err) {
    call StdControl.start();
    if(TOS_NODE_ID == 1) {
      data = 0xBEEF;
      dbg("TestDipP","Updating data items\n");
      /*
      call DisseminationUpdate1.change(&data);
      */
